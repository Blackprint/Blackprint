## html
<sf-space blackprint>
  <sf-m name="container"
    class="{{ _isImporting ? 'importing-nodes' : '' }}"
    @mousewheel="scaleContainer(event)"
    @touchstart.capture="checkTouch(event)"
    @pointerdown.capture="recheckOffset(event)"
    style="
      min-width: {{ size.w }}px;
      min-height: {{ size.h }}px;
      transform: translate3d({{ pos.x }}px, {{ pos.y }}px, 0px)
                 scale({{ scale }})"
  >
    <!-- Put the cables behind the nodes -->
    <sf-m name="cables" class="cables">
      <!-- Put empty space event here, because this element filling the empty space -->
      <!-- recalculatePath: ./container/cables.js -->
      <!-- list: ./container/nodes.js -->
      <svg
        @contextmenu.stop.prevent="container.nodeScope.menu(event)"
        @pointerover.capture="container.nodeScope.pointerOut(event)"
        @pointerdown="container.moveContainer(event)"
      >
        <g sf-each="x in list" class="
          {{ x.typeName }}
          {{ x.valid ? '' : 'invalid' }}
          {{ x.connected ? 'connected' : ''}}
          {{ x.disabled ? 'disabled' : ''}}
          {{ x.hasBranch ? 'has-branch' : ''}}
          {{ x.selected ? 'selected' : ''}}
          {{ x._inactive ? 'inactive' : '' }}
        " @contextmenu.capture.prevent="x.cableMenu(event)">
          <path
            @pointerdown.capture="x.cablePathClicked(event)"
            d="M {{ recalculatePath(x) || x.head1[0] + ' ' + x.head1[1] }}
               C {{ x.linePath }},
                 {{ recalculatePath(x) || x.head2[0] + ' ' + x.head2[1] }}"
          ></path>
          <circle
            @pointerdown.capture="x.cableHeadClicked(event)"
            transform="translate({{ x.head2[0] }}, {{ x.head2[1] }})"
          ></circle>
        </g>
      </svg>
      <div class="glow-cable"></div>
    </sf-m>

    <!-- Nodes goes here -->
    <sf-m name="nodes" class="nodes" style="user-select:none;">
      <!-- Handled on ./nodes.js -->
      <div sf-each="item in list" class="{{ item._nodeSelected ? 'selected' : '' }} {{ item._inactive ? 'inactive' : '' }}"
        @pointerover.capture="pointerOver(event, item)"
        @pointerup="checkNodeClick(event)">
        {{@exec
          // Capitalize each word
          var name = item.interface.split('.sf')[0].replace(/[\/.,<>:\[\]{}+_=`~!@#$%^*(\\|)]/g, '-').toLowerCase();
          var node = sf.component.roots[name];

          if(node === void 0){
            throw new Error("Node element with interface '"+name+"' ("+item.interface+") was not found");
            return null;
          }

          // Create from Blackprint's namespace and (true = let the item become the scope)
          Blackprint._reuseIFace = item;
          return new node(item, $space);
        }}
      </div>
    </sf-m>

    <div class="select {{ select.show ? '' : 'hide' }}" style="
      height: {{ select.h }}px;
      width: {{ select.w }}px;
      transform: translate3d({{ select.x }}px, {{ select.y }}px, 0px)
                 scale({{ select.ix ? '-':'' }}1, {{ select.iy ? '-':'' }}1);
    "></div>
  </sf-m>

  <sf-m name="dropdown">
    <drop-down sf-each="val in menus"></drop-down>
  </sf-m>
</sf-space>

## scss-global
sf-space[blackprint]{
  position: absolute;

  sf-m[name=container] {
    > .select {
      background: #529fff40;
      outline: 1px solid #0095ff;
      position: absolute;
      backface-visibility: hidden;
      top: 0;
      left: 0;
      display: block;
      transform-origin: top left;

      &.hide {
        display: none;
      }
    }
  }

  &, .nodes, .cables{
    height: 100%;
    width: 100%;
    display: block;
  }

  .description{
    margin-top: 3px;
    white-space: pre-line;
  }

  .node{
    textarea, input{
      background: rgba(0, 0, 0, 0.63);
      color: white;
      border: none;
    }
    textarea{
      margin: 0px 5px 0px -10px;
    }

    select{
      background: #232323;
      color: white;
      border: 1px solid #6d6d6d;
    }

    &.input{
      textarea{
        margin-left: 5px;
        margin-right: -10px;
      }
    }
  }

  .BP-Hide{
    display: none;
  }

  .BP-AllowResync {
    color: yellow;
  }

  @mixin portType($type, $color) {
    .cables > svg > g.#{$type} {
      circle{fill: #{$color}}
      path{stroke: #{$color}}
    }

    .nodes .node .ports.#{$type} {
      & > .port {
        background: #{$color};
        // color: #{$color};
      }

      comp-port-input input {
        border: 1px solid #{$color}8c;
        box-shadow: 0 0 2px #{$color}8c;
      }

      &.inactive {
        color: grey;
        & > .port {
          background: transparent;
          border: 2px solid #{$color};
          width: 8px;
          height: 8px;
          box-shadow: 0 0 4px #{$color};
        }
      }
    }
  }

  @include portType("Number", #00bfff);
  @include portType("Object", #9370db);
  @include portType("Boolean", #ff5959);
  @include portType("String", #55ff52);
  @include portType("BP-Union", #ffffff);
  @include portType("BP-Route", #ffac7b);

  // BP-Route
    .nodes .node .ports.BP-Route > .port {
        border: 2px solid #ffac7b;
        border-radius: 3px;
        transform: rotate(45deg);
        height: 7px;
        width: 7px;
    }
  // BP-Route

  // BP-StructOf
  .nodes .node .ports {
    &.BP-StructOf {
      & > .port {
          overflow: hidden;
          &:after {
            content: '';
            background: #ffffff;
            width: 100%;
            height: 100%;
            transform: translate(4px, -4px) rotate(45deg);
            outline: 2px black dashed;
          }
      }
      &.BP-Open {
        display: none; // ToDo: remove me
        margin: 10px 10px 0 0;
        .port{
          display: none;
        }
      }
    }
    &.BP-StructSplit.BP-Last {
      margin-bottom: 10px;
    }
  }
  // BP-StructOf

  .cables > svg > g.BP-Route path {
    stroke-dasharray: 10px;
    stroke: orange;
    stroke-linecap: round;
  }

  .cables .inactive{
    opacity: 0.4;
  }

  .glow-cable{
    position: absolute;
    top: 0;
    left: 0;
  }

  .bp-dot-glow{
    position: absolute;
    width: 8px;
    height: 8px;
    border-radius: 5px;
    box-shadow: 0 0 10px yellow;
    transform: translate(-5px, -5px); // Hide it first on out of bound container
    background: yellow;
    visibility: hidden;
    // &::after{
    //   content: '🍒';
    // }
  }

  .nodes .node .ports.ArrayOf > .port {
    background: none;
    font-family: "Font Awesome 5 Free";
    font-weight: 900;
    font-size: 12px;

    &:before{
      content: "\f00a";
      margin-top: -1px;
      position: absolute;
    }
  }

  sf-m[name="container"]{
    transform-origin: top left;
    backface-visibility: hidden;
    touch-action: pinch-zoom;
  }

  .nodes{
    .node:before{
      left: -1px;
      z-index: -1;
      content: '';
      box-shadow: 0px 0px 20px white;
      background: #ffffff12;
      width: 100%;
      border-radius: 6px;
      border: 1px solid black;
      height: 100%;
      top: -1px;
      position: absolute;
      opacity: 0;
      transition: opacity 0.3s ease-in;
    }

    .highlight .node:before{
      opacity: 1;
    }

    .routes div {
      transform: rotate(45deg);
      background: #ffac7b;
      width: 10px;
      border-radius: 3px;
      box-shadow: 0 0 3px 0 black;
      height: 10px;
      border: 1px solid black;
      position: absolute;
      top: -7px;
      &.in { left: -7px; }
      &.out { right: -7px; }
      &.hide { opacity: 0; }
      &.disable { display: none; }
    }
    .routes.show div{
      z-index: 1;
    }
    .routes.show div.hide{
      opacity: 1;
    }

    .routes.no-update div {
      &.in { background: #727272; }
    }
    .routes.disabled div {
      display: none;
    }
  }

  .cables{
    position: absolute;
    > svg{
      // Apply only on direct child of .cables
      filter: drop-shadow(0 0 2px black);
      backface-visibility: hidden;
      height: 100%;
      width: 100%;
      transform: translateZ(0px);

      path, circle{
        transition: 0.1s ease-in;
        transition-property: stroke, fill;
      }

      .connected circle{
        display: none;
      }

      .has-branch circle{
        display: block;
      }

      g.selected circle{
        r: 8;
        fill: white;
      }

      .disabled{
        opacity: 0.4;
      }

      circle{
        r: 5;
        fill: orange;
      }

      path{
        stroke: orange;
        stroke-width: 3;
        fill: none;
      }

      .invalid{
        circle{fill: red !important; opacity: 0.6;}
        path{stroke: red !important; opacity: 0.6;}
      }

      g:hover{
        circle{fill: white !important;}
        path{stroke: white !important;}
      }

      .highlight{
        path{
          stroke: white;
        }
        circle{
          fill: white;
        }
      }
    }
  }

  .nodes{
    position: unset;

    .selected .node {
      border: 1px solid white;
      box-shadow: 0 0 5px white;
    }

    .inactive .node {
      filter: saturate(0) opacity(0.7);
      &:hover{
        filter: none;
      }
    }

    .node{
      color: white;
      font-family: Arial;
      border-radius: 5px;
      border: 1px solid black;
      box-shadow: 0 0 5px black;
      position: absolute;
      background: rgba(0,0,0,0.7);
      backface-visibility: hidden;
      transform: translateZ(0px);

      @mixin headerColor($type, $borderColor, $bgA, $bgB, $bgC) {
        &.#{$type} .header{
          box-shadow: inset 0 0 3px $borderColor;
          background: linear-gradient(120deg, $bgA 0%, $bgB 80%, $bgC 100%);
        }
      }

      @include headerColor('event', #a05f5f, rgba(226, 18, 18, 0.7), rgba(229, 41, 41, 0.2), rgba(229, 32, 32, 0.1));
      @include headerColor('input', #75a05f, rgba(108, 232, 93, 0.7), rgba(103, 255, 108, 0.2), rgba(130, 226, 124, 0.1));
      @include headerColor('func-in-out', #b030ff, #9600c7b3, #42006ab3, #8332dbb3);

      .header{
        display: block;
        text-align: left;
        padding: 3px 5px 2px 5px;
        border-radius: 5px 5px 0 0;
        box-shadow: inset 0 0 3px cadetblue;
        background: linear-gradient(120deg, rgba(41,184,229,0.7) 0%, rgba(41,184,229,0.2) 80%, rgba(32,124,229,0.1) 100%);

        /*linear-gradient(160deg, rgba(41,184,229,0.7) 0%, rgba(41,184,229,0.2) 54%, rgba(32,124,229,0.1) 80%, rgba(32,124,229,0.2) 100%)

        -webkit-gradient(
          linear, left top, right bottom, from(rgba(41,184,229,0.7)),
          to(rgba(41,184,229,0.2)), color-stop(.8,rgba(32,124,229,0.1))
        );*/

        .description{
          font-size: 12px;
          color: darkgray;
          padding-right: 5px;
          font-style: italic;
        }

        .extra{
          position: absolute;
          top: -12px;
          right: -10px;
          .item{
            background: black;
            border-radius: 50px;
            box-shadow: 0 0 11px white;
            height: 20px;
            width: 20px;
            padding: 2px;
            font-size: 12px;
            text-align: center;
            display: inline-block;
            margin-left: 5px;

            i{
              line-height: 21px;
            }

            &.node-route {
              font-size: 15px;
              line-height: 20px;
              margin-left: 10px;
              box-shadow: 0 0 17px 5px #ff8d00e3;
              background: #53340e;
              outline: 2px solid #ff8d00;
            }
          }
        }

        &.small{
            position: absolute;
            height: calc(100% - 5px);
            border-radius: 5px 0 0 5px;
        }
      }

      .content{
        display: inline;
        width: 100%;

        &.header-small{
          margin-left: 32px;
        }
      }

      .other {
        position: absolute;
        bottom: calc(100% - 15px);
        display: grid;
        left: -1px;
        justify-items: center;
        right: -1px;
        z-index: -1;

        .list{
          display: grid;
        }

        .item {
          display: inline-flex;
          margin-top: 10px;

          &.comment {
            border-radius: 5px;
            width: calc(100% - 12px);
            background: #0000009e;
            outline: 1px solid black;
            box-shadow: 0 0 5px white;
            padding: 5px;
            bottom: 25px;
            font-size: 14px;
            position: relative;
          }
        }
      }

      .ports{
        &.inactive {
          color: grey;
          & > .port {
            background: transparent;
            border: 2px solid white;
            width: 8px;
            height: 8px;
            box-shadow: 0 0 4px white;
          }
        }
        &.unused {
          display: none;
        }

        .port{
          z-index: 200;
          background: white;
          height: 10px;
          width: 10px;
          border-radius: 100px;
          border: 1px solid black;
          box-shadow: 0 0 5px black;

          &.event{
            border-radius: 2px;
            transform: rotate(45deg) skew(10deg, 10deg);
          }
        }

        &._Exec {
          .port {
            background: white;
            clip-path: path('M 0 0 L 6 0 C 8 1 12 5 12 6 C 12 7 8 11 6 12 L 0 12 C 3 9 2 10 5 6 C 3 3 2 2 0 0');
            width: 12px;
            height: 12px;
            border-radius: 0;

            border: none;
            box-shadow: none;
          }
        }

        &.Function{
          height: 20px;
          .name{
            line-height: 20px;
          }
          .port{
            background: url(data:image/png;base64,);
            background-size: 20px;
            width: 20px;
            height: 20px;

            border: none;
            box-shadow: none;
          }
        }
      }

      .output, .input{
        margin-top: 1px;

        // > = direct child
        .ports > div {
          display: inline-flex;
        }
        .ports > .name {
          margin: 0 3px 0 5px;
        }
      }

      .left-port{
        float: left;
      }
      .right-port{
          float: right;
          text-align: right;
          margin-left: 20px;
      }

      .output{
        margin-right: 5px;
        .name{
          text-align: right;
        }

        .Function .port{
          float: right;
          margin-left: -1px;
          margin-right: -5px;
        }
      }

      .input{
        padding-right: 20px;
        margin-left: 5px;
        white-space: nowrap;

        .Function .port{
          float: left;
          margin-left: -2px;
        }

        input::-webkit-outer-spin-button,
        input::-webkit-inner-spin-button {
          -webkit-appearance: none;
          margin: 0;
        }

        /* Firefox */
        input[type=number] {
          -moz-appearance: textfield;
        }
      }

      .property{
        display: flex;
        justify-content: center;
        margin-top: 5px;
        margin-bottom: 5px;
        .ports{
          margin-right: 8px;
          margin-left: 8px;
          .port{
            margin: 0 auto;
          }
        }
      }
    }
  }
}

body.blackprint-no-vfx sf-space[blackprint], body sf-space[blackprint] .importing-nodes {
  .cables > svg{
    filter: none;
  }
  .nodes .node{
    transition: none;
  }
  .nodes * {
    box-shadow: none !important;
  }
}

body sf-space[blackprint] .importing-nodes{
  pointer-events: none;
}